<div class="container-fluid">
  <div class="embed-responsive embed-responsive-16by9">

    <div class="searchbox">
      <%= form_for :search, url: searches_path do |f| %>
      <p>
        <%= f.label :search_city %><br>
        <%= f.text_field :search_city %>
      </p>
      <p>
        <%= f.label :search_price %><br>
        <%= f.text_field :search_price %>
      </p>
      <p>
        <%= f.label :search_bedroom %><br>
        <%= f.text_field :search_bedroom %>
      </p>
      <p>
        <%= f.label :search_period %><br>
        <%= f.text_field :search_period %>
      </p>
      <p>
        <%= f.submit %>
      </p>
      <% end %>
    </div>

    <iframe id='player' width="2500" height="1406" src="https://www.youtube.com/embed/FarSjtiP3Sc?rel=0&amp;controls=0&amp;showinfo=0&amp;autoplay=1&amp;loop=1&amp;playlist=FarSjtiP3Sc&amp;enablejsapi=1&version=3" frameborder="0">  <div id="background">
    </iframe>
  </div>
</div>
</div>



