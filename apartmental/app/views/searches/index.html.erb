<div id="container-search">

  <div class="top-bar">
    <nav class="top-bar" data-topbar role="navigation">
      <ul class="title-area">
        <li class="name">
          <h1><a href="/">My Site</a></h1>
        </li>
      </ul>
    </nav>
  </div>

  <div class="small-6 columns" id="left_column">
    <div class='front'>
      <div class="row" id="pop_up">
        <div class="search-partial">
          <button class="research">Search for a new apartment!</button>
        </div>
      </div>
      <div class="row" id="left_up" data-search ="<%= @sorted_results.values[0]["search_id"] %>" data-address="<%= @sorted_results.values[0]["address"].id%>">
        <%= render "search_bar" %>
        <div id="tile-wrapper">
          <div>You got <%= @sorted_results.count %> results! </div>
          <% @sorted_results.each_pair do |key, value_hash| %>

          <div id="front">
            <div class="tile small-5 columns" id="<%=value_hash["address"].id%>" data="<%=value_hash["search_id"]%>">
              <!-- <%= key %> -->
              <div class='tile-score tile-score<%=value_hash["address"].id%>'><%= key.round(2) %></div>
              <div class="layer">
                <img id="tile-image" class="tile-image<%=value_hash["address"].id%>" src="<%= value_hash["address"].image_url %>"></img>
                <div id="tile-desc"><%= value_hash["address"].street_number %> <%= value_hash["address"].street_name %> <%= value_hash["address"].street_type %></div>

              </div>
            </div>
          </div>
          <% end %>

        </div>
      </div>
    </div>

    <div class='back'>
      <div id='back-container'>
        <button id='flip-back'></button>
        <% @sorted_results.each_pair do |key, value_hash| %>

        <div class="title" id="title<%=value_hash["address"].id%>" style="display: none">
          <h1><%= value_hash["address"].street_number %> <%= value_hash["address"].street_name %> <%= value_hash["address"].street_type %></h1>
        </div>
        <div class='row'>
          <div class="description large-8 columns" id="description<%=value_hash["address"].id%>" >
            <%= value_hash["address"].street_number %> <%= value_hash["address"].street_name %> <%= value_hash["address"].street_type %>
          </div>
          <div class="image large-4 columns" id="image<%=value_hash["address"].id%>">
            <img src="<%= value_hash["address"].image_url %>"</img>
          </div>
        </div>
        <% end %>
      </div>
    </div>
  </div>
    <div class="small-6 columns" id="right_column">
      <div class="row" id="map" name="<%=@sorted_results.values[0]["search_id"]%>"></div>
      <div class="row" id="right_down">
        <div class="chart-description">
          Key: <BR>
          R = Rent <BR>
          WS = Walk Score <BR>
          CR = Crime Rating <BR>
          CT = Commute Time <BR>
        </div>
        <canvas id="myChart" width="400" height="100%"></canvas>
      </div>

    </div>
>>>>>>> master

  </div>
</div>



